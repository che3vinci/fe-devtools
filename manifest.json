{

  "manifest_version": 3,

  "version": "0.0.0",
  "author": "chevinci",
  "name": "DEV | Vis inspect",
  "description": "A basic Plasmo extension.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/index.90e04114.js"
      ],
      "css": [
        "assets/index.90e04114.css"
      ]
    }
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' http://localhost;object-src 'self';"
  }
}
